{
    "name": "raiderknight/rkextendsocial",
    "type": "phpbb-extension",
    "description": "This extension allows you to add some basic Font Awesome socials to the forum footer, the social links can be set up via the ACP.",
    "version": "1.0.0",
    "time": "2022-02-01",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "RaiderKnight",
            "homepage": "https://raiderknight.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "phpbb/phpbb": ">=3.3.4",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Extended Footer Socials",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.4"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/rkextendsocial",
            "filename": "version_check",
            "ssl": true
        }
    }
}